<div class="skin-mirage-visible-content">
	<div class="skin-mirage-main-container">
		{{# html-right-rail-collapse-button }}
			<input
				type="checkbox"
				id="mirage-right-rail-checkbox"
				class="mw-checkbox-hack-checkbox"
				{{# is-right-rail-visible }}checked{{/ is-right-rail-visible }}>
		{{/ html-right-rail-collapse-button }}
		{{# data-header }}
			{{> Header }}
		{{/ data-header }}
		<div class="skin-mirage-content-container">
			{{# html-site-notice }}
				<div id="siteNotice">{{& . }}</div>
			{{/ html-site-notice }}
			<div class="mediawiki-content-container">
				{{! This specifies role="main" for IE compatibility. }}
				<main id="content" class="mw-body" role="main">
					<a id="top"></a>
					{{> Indicators }}
					{{& html-title-heading }}
					<div id="bodyContent">
						{{# is-article }}<div id="siteSub" class="noprint">{{ msg-tagline }}</div>{{/ is-article }}
						<div id="contentSub" {{& html-user-language-attributes }}>{{& html-subtitle }}</div>
						{{# html-undelete-link }}<div id="contentSub2">{{& . }}</div>{{/ html-undelete-link }}
						<a class="mw-jump-link" href="#mw-navigation">{{ msg-mirage-jumptonavigation }}</a>
						{{& html-body-content }}
					</div>
					{{& html-after-content }}
				</main>
			</div>
			{{# array-right-rail }}
				<div id="mirage-right-rail">
					{{# array-right-rail-modules }}
						{{> RightRailModule }}
					{{/ array-right-rail-modules }}
				</div>
			{{/ array-right-rail }}
		</div>
	</div>
	{{# data-footer }}
		{{> Footer }}
	{{/ data-footer }}
</div>
